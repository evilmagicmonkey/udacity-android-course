<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.bowerstonetourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MarketActivity"
            android:label="@string/category_market"
            android:parentActivityName=".MainActivity"/>

        <activity android:name=".HomesActivity"
            android:label="@string/category_homes"
            android:parentActivityName=".MainActivity"/>

        <activity android:name=".LandmarksActivity"
            android:label="@string/category_landmarks"
            android:parentActivityName=".MainActivity"/>

        <activity android:name=".GraveyardActivity"
            android:label="@string/category_graveyard"
            android:parentActivityName=".MainActivity"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

    </application>


</manifest>